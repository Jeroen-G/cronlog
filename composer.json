{
    "name": "jeroen-g/cronlog",
    "description": "Allows you to log cron jobs to where *you* want it.",
    "keywords": ["Laravel", "Cronlog", "cronjob", "cron jobs", "log"],
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "Jeroen",
            "email": "jeroengjeroeng@gmail.com"
        }
    ],
    "homepage": "https://github.com/jeroen-g/cronlog",
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JeroenG\\Cronlog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenG\\Cronlog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenG\\Cronlog\\CronlogServiceProvider"
            ]
        }
    }
}
